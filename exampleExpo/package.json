    "@expo/webpack-config": "^0.16.24",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-plugin-inline-dotenv": "^1.6.0",

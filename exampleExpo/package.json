    "expo-status-bar": "~1.3.0",
    "i18next": "^21.8.1",
    "moti": "^0.18.0",
    "native-base": "^3.4.5",
    "patch-package": "^6.4.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.16.9",
    "react-native": "0.68.2",
    "react-native-fast-image": "^8.5.11",

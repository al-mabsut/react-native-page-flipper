    "gh-pages": "^4.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "react-native-svg-transformer": "^1.0.0",
    "typescript": "~4.3.5"
  },
  "resolutions": {
    "@types/react": "~17.0.21"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,html,md,css}": [
      "prettier --write"
